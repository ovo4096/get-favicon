{
  "name": "get-favicon",
  "version": "1.0.0",
  "description": "A Node.js service to fetch website favicons",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "pm2:start": "pm2 start ecosystem.single.config.js",
    "pm2:start:cluster": "pm2 start ecosystem.config.js",
    "pm2:start:prod": "pm2 start ecosystem.single.config.js --env production",
    "pm2:stop": "pm2 stop get-favicon",
    "pm2:restart": "pm2 restart get-favicon",
    "pm2:reload": "pm2 reload get-favicon",
    "pm2:delete": "pm2 delete get-favicon",
    "pm2:logs": "pm2 logs get-favicon",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "favicon",
    "api",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0"
  }
}